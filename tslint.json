{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            false
        ],
        "quotemark": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "no-console": [
            false
        ],
        "no-shadowed-variable": false,
        "only-arrow-functions": false,
        "interface-name" : [true, "never-prefix"],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "max-line-length": [true, 500],
        "no-unused-variable": true
    },
    "rulesDirectory": []
}
